{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "no-console": 0,
    "max-len": [1, 150, 4],
    "react/no-multi-comp": 0,
    "react/jsx-no-bind": 0
  },
  "plugins": [
    "react"
  ]
}
